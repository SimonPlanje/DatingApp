<%- include ("includes/head.ejs") %> <%- include ("includes/header.ejs") %>
<body id="LoginForm"></body>
<main>

  <h1>New user</h1>

<form class="addData" action="/" method="post">

  <label for="firstname">What is your first name?</label>
  <input type="text" id="firstname" name="firstname" />
  <label for="lastname">How old are you?</label>
  <input type="number" id="age" name="age" min="16" max="50"/>
  <label for="city">Where do you live?</label>
  <input type="text" id="city" name="city" />
  <label for="description" name="description">
    Tell us something about you!
  </label>
  <textarea name="description" cols="30" rows="5"></textarea>
  <button>Let's like!</button>
</form>
</main>
</body>